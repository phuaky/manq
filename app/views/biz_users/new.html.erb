<h1>BizUsers#new</h1>
<p>Find me in app/views/biz_users/new.html.erb</p>

<div class="row">
  <form class="col s12" action='/biz_users' method='post'>
    <div class="row">
      <div class="input-field col s12 m4">
        <input placeholder="abc@email.com" id="email" type="text" class="validate" name="registered_user[email]">
        <label for="email" class="active">Email</label>
      </div>
      <div class="input-field col s12 m4">
        <input id="password" type="password" class="validate" name="registered_user[password]">
        <label for="password" class="active">Password</label>
      </div>
      <div class="input-field col s12 m4">
        <input id="password_confirmation" type="password" class="validate" name="registered_user[password_confirmation]">
        <label for="password_confirmation" class="active">Confirm Password</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12 m4">
        <input id="reg_no" type="text" class="validate" name="biz_user[reg_no]">
        <label for="reg_no" class="active">Business Registration Number</label>
      </div>
      <div class="input-field col s12 m4">
        <input id="company_name" type="text" class="validate" name="biz_user[company_name]">
        <label for="company_name" class="active">Company Name</label>
      </div>
      <div class="input-field col s12 m4">
        <input id="address" type="text" class="validate" name="biz_user[address]">
        <label for="address" class="active">Company Address</label>
      </div>
    </div>
    <div class="row">
      <div class="input-field col s12 m6">
        <input id="contact_person" type="text" class="validate" name="biz_user[contact_person]">
        <label for="contact_person" class="active">Contact Person</label>
      </div>
      <div class="input-field col s12 m6">
        <input id="contact_no" type="text" class="validate" name="biz_user[contact_no]">
        <label for="contact_no" class="active">Contact No</label>
      </div>
    </div>
    <button class="btn waves-effect waves-light right" type="submit" name="action">Submit
      <i class="material-icons right">play_arrow</i>
    </button>
  </form>
</div>
