<h1>Stores#new</h1>
<p>Find me in app/views/stores/new.html.erb</p>
<form action="/stores" class="ui form" method="post">
  <div class="field">
    <div>
      <label for="store_name">Store Name</label>
      <input type="text" id="store_name" name="store[store_name]">
    </div> <br>

    <div>
      <label for="store_address">Store Address...</label>
      <input type="text" id="store_address">
    </div> <br>

    <!-- <div>
      <label for="store_picture"></label>
      <input type="file" name="store[picture]" value="store_picture">
    </div> -->

    <div>
      <label for="contact_person">Contact Person...</label>
      <input type="text" id="contact_person">
    </div> <br>

    <div>
      <label for="contact_no">Contact no...</label>
     <input type="text" pattern="-?[0-9]*(\.[0-9]+)?" id="contact_no">
    </div> <br>

    Max Queue No.
    <input type="range"
    min="0" max="100" value="25" tabindex="0">

    Max Lee way.
    <input type="range"
     min="0" max="5" value="3" tabindex="0">

     <div>
       <label for="reservation_fee">Reservation Fee...</label>
      <input type="text" pattern="-?[0-9]*(\.[0-9]+)?" id="reservation_fee">
    </div> <br>

     <button class="ui button" type="submit">Submit</button>
  </div>


</form>
