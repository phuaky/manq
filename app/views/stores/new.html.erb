<form action="/stores" class="ui small form" method="post">
  
<div class="ui one column centered grid">
  <div class="ui one wide left aligned container">
    <h1>Stores#new</h1>
    <div class="ui centered row">
      <div class="field">  
        <div class="ui blue small label">
          <label>Store Name</label>
        </div> <br>
        <div class="ui small input">
          <input style="width:200px; height: 22px;" type="text" id="store_name" name="store[store_name]">
        </div> <br>
      </div>
      <div class="field">  
        <div class="ui blue small label">
          <label>Store Address</label>
        </div> <br>
        <div class="ui small input">
          <input style="width:200px; height: 22px;" type="text" id="store_address">
        </div> <br>
      </div>
      <!--<div class="field">  -->
      <!--  <div class="ui blue small label">      -->
      <!--    <label>Store Picture"></label>-->
      <!--  </div> <br>-->
      <!--  <div class="ui small input">         -->
      <!--    <input style="width:200px; height: 22px;" type="file" name="store[picture]" value="store_picture">-->
      <!--  </div> <br>-->
      <!--</div>-->
      <div class="field">  
        <div class="ui blue small label">    
          <label>Contact Person</label>
        </div> <br>
        <div class="ui small input">        
          <input style="width:200px; height: 22px;" type="text" id="contact_person">
        </div> <br>
      </div>
      <div class="field">  
        <div class="ui blue small label">    
          <label>Contact No</label>
        </div> <br>
        <div class="ui small input">        
          <input style="width:100px; height: 22px;" type="text" pattern="-?[0-9]*(\.[0-9]+)?" id="contact_no">
        </div> <br>
      </div
      <div class="field">  
        <div class="ui blue small label">    
          <label>Max Queue No</label>
        </div> <br>
        <div class="ui small input">
          <input style="width:200px; height: 22px;" type="text" type="range"
          min="0" max="100" value="25" tabindex="0">
        </div> <br>
        <div class="ui blue small label">    
          <label>Max Leeway</label>
        </div> <br>
       <div class="ui small input">
          <input style="width:200px; height: 22px;" type="text" type="range"
           min="0" max="5" value="3" tabindex="0">
        </div> <br>
      </div>
      <div class="field">  
        <div class="ui blue small label">    
           <label for="reservation_fee">Reservation Fee...</label>
        </div> <br>
        <div class="ui small input">       
           <input style="width:200px; height: 22px;" type="text" pattern="-?[0-9]*(\.[0-9]+)?" id="reservation_fee">
        </div> <br>
      </div>
      <div class="field">  
        <div class="ui small buttons">
          <button class="ui green button" type="submit">Submit</button>
        </div>
      </div>
    </div>
  </div>
</div>
</form>
